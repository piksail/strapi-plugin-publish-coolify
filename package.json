{
  "name": "@piksail/strapi-plugin-publish-coolify",
  "version": "0.1.1",
  "description": "Strapi plugin to publish your content to Coolify",
  "keywords": [
    "strapi",
    "plugin",
    "coolify",
    "deployment",
    "ssg",
    "static-site"
  ],
  "type": "commonjs",
  "license": "MIT",
  "author": {
    "name": "Piksail",
    "email": "dev@piksail.fr",
    "url": "https://piksail.fr"
  },
  "maintainers": [
    {
      "name": "Piksail",
      "email": "dev@piksail.fr",
      "url": "https://piksail.fr"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/piksail/strapi-plugin-publish-coolify.git"
  },
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "admin",
    "server"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "tsc -p admin/tsconfig.json",
    "test:ts:back": "tsc -p server/tsconfig.json"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.31.3",
    "@strapi/typescript-utils": "^5.31.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "prettier": "^3.7.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.31.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "publish-coolify",
    "displayName": "Coolify Publisher",
    "description": "Trigger Coolify deployments from Strapi"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}
